<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite-LoginTestCases">

		<listeners> 
        	<listener class-name="MercadoLibre.Listeners"/>
        </listeners>
          
  <test thread-count="5" name="Login data driven">
    <classes>
      <class name="MercadoLibre.LoginDataDrivenTestCase"/>
    </classes>
  </test>
  
 <test thread-count="5" name="Login multiple users">
    <classes>
       <class name="MercadoLibre.LoginMultipleUsersTestCase"/>
    </classes>
  </test>  
  
  <test thread-count="5" name="Login with errors"> 
    <classes>
      <class name="MercadoLibre.LoginWithErrorsTestCase"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
